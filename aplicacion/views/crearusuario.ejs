<h1>Crear Usuario</h1>

<form action="/Usuario/crearUsuarioQuemado" method="post">


  <div class="form-group row">
    <label for="nombres" class="col-2 col-form-label">Nombres</label>
    <div class="col-10">
      <input class="form-control"
             type="text"
             placeholder="Ingrese sus nombres"
             id="nombres"
             name="nombres"

      >
    </div>
  </div>

  <div class="form-group row">
    <label for="password" class="col-2 col-form-label">Password</label>
    <div class="col-10">
      <input class="form-control" type="password" placeholder="Ingrese un password"
             name="password"
             id="password">
    </div>
  </div>



  <button type="button"
          class="btn btn-outline-secondary">Boton</button>

  <button
          class="btn btn-outline-success">Crear</button>

  <button type="reset"
          class="btn btn-outline-warning">Reset</button>

</form>
